config_version=5

[application]

config/name="Argos"
run/main_scene="res://ship_map.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

Engineer="*res://Global/NPC/Engineer.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

